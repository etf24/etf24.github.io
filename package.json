{
    "name": "etf24-app",
    "private": true,
    "version": "0.1.2",
    "type": "module",
    "scripts": {
        "dev": "vite dev --config frameworks/config/vite.config.ts",
        "build": "vite build --config frameworks/config/vite.config.ts",
        "preview": "vite preview --config frameworks/config/vite.config.ts",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test": "vitest run --config frameworks/config/vitest.config.ts",
        "test:watch": "vitest --config frameworks/config/vitest.config.ts",
        "test:e2e": "playwright test",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@playwright/test": "^1.58",
        "@sveltejs/adapter-static": "^3.0",
        "@sveltejs/kit": "^2.52",
        "@sveltejs/vite-plugin-svelte": "^6.2",
        "@tailwindcss/vite": "^4.1",
        "@types/papaparse": "^5.5",
        "@vite-pwa/sveltekit": "^1.1",
        "eslint": "^10.0",
        "eslint-config-prettier": "^10.1",
        "eslint-plugin-svelte": "^3.15",
        "prettier": "^3.8",
        "prettier-plugin-svelte": "^3.4",
        "svelte": "^5.51",
        "svelte-check": "^4.4",
        "tailwindcss": "^4.1",
        "typescript": "^5.9",
        "vite": "^7.3",
        "vitest": "^4.0"
    },
    "dependencies": {
        "@fontsource/lato": "^5.2.7",
        "@inlang/paraglide-sveltekit": "^0.16.1",
        "ag-grid-community": "^35.1",
        "dexie": "^4.3",
        "echarts": "^6.0",
        "lucide-svelte": "^0.564.0",
        "papaparse": "^5.5"
    }
}
